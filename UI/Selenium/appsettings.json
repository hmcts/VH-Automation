{
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "async": true,
      "logfile": {
        "type": "File",
        "fileName": "${configsetting:item=SystemConfiguration.LogLocation}/log.${longdate:cached=true}-${configsetting:item=SystemConfiguration.LogSuffix}.log",
        "layout": "${longdate} ${uppercase:${level}} ${message}",
        "archiveFileName": "${basedir}/logs/archives/log.${shortdate}.{#}.log",
        "archiveAboveSize": 5242880,
        "archiveEvery": "Day",
        "archiveNumbering": "Rolling",
        "maxArchiveFiles": 20
      },
      "logconsole": {
        "type": "Console"
      }
    },
      "rules": [
        {
          "logger": "*",
          "minLevel": "Info",
          "writeTo": "logconsole"
        },
        {
          "logger": "*",
          "minLevel": "Debug",
          "writeTo": "logfile"
        }
      ]
    },
